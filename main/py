def divide_numbers(a, b):
    try:
        result = a / b
        return result
    except ZeroDivisionError:
        print("Помилка: ділення на нуль.")
        return None


num1 = 10
num2 = 0
result = divide_numbers(num1, num2)

if result is not None:
    print(f"Результат ділення {num1} на {num2} дорівнює {result}")
